<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/tronalddump_850x850"
        android:label="Trump"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TrumpDumpAPI"
        tools:targetApi="31">
        <activity
            android:name=".ActivitySplashScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.trumpdumpapi.WelcomeActivity"
            android:label="Fragment First Screen"
            />

        <activity android:name="com.example.trumpdumpapi.AcivityAboutUs"
          android:label="Fragment Second Screen"
            />

        <activity
            android:name=".MainActivity"
            android:exported="true">


        </activity>
    </application>

</manifest>